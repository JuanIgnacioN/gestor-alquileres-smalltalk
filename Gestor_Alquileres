<class-id>VentanaInmueblesPersona</class-id> <category>As yet unclassified</category>

<body package="Gestor_Alquileres" selector="clickExportar">clickExportar	| dataStream textoInmuebles |	textoInmuebles := String new.	textoInmuebles := 'ID' , ';' , 'ID Propietario' , ';' , 'ID Calle' , ';'				, 'numero' , ';'				, 'piso' , ';'				, 'departamento' , ';'				, 'tipo' , ';'				, 'superficieTotal' , ';'				, 'superficieCubierta' , ';'				, 'cantidadHabitaciones' , ';'				, 'garage' , ';'				, 'cantidadBaños'.	dataStream := 'C:\paradigmas\ListadoInmueblesPersona.csv' asFilename writeStream.	dataStream nextPutAll: textoInmuebles.	dataStream cr.		contenedor do: 			[:e |			textoInmuebles := e id printString , ';' , e idPropietario printString						, ';' , e idCalle printString						, ';' , e numero printString						, ';' , e piso printString						, ';' , e departamento printString						, ';' , e tipo printString						, ';' , e superficieTotal printString						, ';' , e superficieCubierta printString						, ';' , e cantidadHabitaciones printString						, ';' , e garaje printString						, ';' , e cantidadBaños printString.			dataStream nextPutAll: textoInmuebles.			dataStream cr].		dataStream close.		Dialog warn:'Se ha exportado el archivo con exito'.</body>

<body package="Gestor_Alquileres" selector="clickMostrar">clickMostrar| aux  wa |	aux:=Administrador  new.	(dni value ~= 0)	ifFalse:[contenedor:= aux inmuebles]	ifTrue:[		wa := aux inmuebles detect:[:a| a idPropietario = (dni value) ] ifNone: [dni value: 0.		wa:=nil].			(wa isNil) ifFalse:[      contenedor:= aux inmuebles select:[:b| b idPropietario = wa idPropietario]]].	self actualizarTabla.</body>
</methods>

