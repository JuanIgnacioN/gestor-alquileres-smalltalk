<body package="Gestor_Alquileres" selector="buscarIdPersonaConNombre:">buscarIdPersonaConNombre: nom| resultado|	personas do:[:a| (a nombre = nom) ifTrue:[resultado:= a id]].^resultado</body>

<body package="Gestor_Alquileres" selector="rankingInquilinosAtr">rankingInquilinosAtr| aux |aux:= personas asSortedCollection: [:a : b| (self cantAlqAtr: a )&lt;(self cantAlqAtr:b )].^aux</body>

