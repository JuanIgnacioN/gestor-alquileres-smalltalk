<class-id>AdmPersonas</class-id> <category>modificar</category>

<body package="Gestor_Alquileres" selector="clickModPersona">clickModPersona	| persona  vp aux seleccion fila|		aux:=Administrador new.	persona := Persona new.	vp := VentanaPersonas new. 		fila:=(aspectoTabla selectionIndex)value y.	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida'] ifFalse:[			vp persona: persona.	seleccion:= aspectoTabla table at:(3@fila ).	 vp contPersona value:(aux buscarPersona: seleccion ) .	vp open.	]</body>

<body package="Gestor_Alquileres" selector="abrirModificarPersona">abrirModificarPersona| aux |	aux:=VentanaModificarPersona  new.	aux open.	self aspectoTabla selection copy as value.	^self closeRequest</body>
</methods>

