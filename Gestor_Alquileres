<body package="Gestor_Alquileres" selector="volver">volver| aux |	aux:=MenuPrincipal new.	aux open.	^self closeRequest</body>

<body package="Gestor_Alquileres" selector="clickMostrar">clickMostrar|a  temp aux|aux:=Administrador new.(fechaInicio value isNil)ifTrue: [Dialog warn: 'Ingrese correctamente las fechas.']ifFalse:[a:= aux cobros select: [:rec | rec fecha between: fechaInicio value and: fechaFin value].temp:= a collect: [:e | e idContrato].contenedor:= aux contratos select: [:e| temp includes: e id ].self actualizarTabla.]</body>

<body package="Gestor_Alquileres" selector="aspectoTabla">aspectoTabla	"This method was generated by UIDefiner.  Any edits made here	may be lost whenever methods are automatically defined.  The	initialization provided below may have been preempted by an	initialize method."	^aspectoTabla isNil		ifTrue:			[aspectoTabla := TableInterface new selectionInTable: tablaContratos ]		ifFalse:			[aspectoTabla]</body>

<body package="Gestor_Alquileres" selector="clickExportar">clickExportar	| dataStream textoContratos |	textoContratos := String new.	textoContratos := 'ID' , ';' , 'ID Inmueble' , ';' , 'ID Inquilino' , ';'				, 'ID Garante' , ';'				, 'Fecha Inicio' , ';'				, 'Fecha FIn' , ';'				, 'Fecha Rescicion' , ';'				, 'Monto' , ';'				, 'PPV'.										dataStream := 'C:\paradigmas\ListadoAEF.csv' asFilename writeStream.	dataStream nextPutAll: textoContratos.	dataStream cr.		contenedor do: 			[:e |			textoContratos := e id printString , ';' , e idInmueble printString						, ';' , e idInquilino printString						, ';' , e idGarante printString						, ';' , e fechaInicio printString						, ';' , e fechaFin printString						, ';' , e fechaRescision printString						, ';' , e monto printString						, ';' , e porcentajeDiarioPunitario printString.									dataStream nextPutAll: textoContratos.			dataStream cr].		dataStream close.	Dialog warn:'Se ha exportado el archivo con exito'.</body>

