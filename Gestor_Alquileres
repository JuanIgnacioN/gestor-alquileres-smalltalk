<body package="Gestor_Alquileres" selector="windowSpec">windowSpec	"Tools.UIPainter new openOnClass: self andSelector: #windowSpec"	&lt;resource: #canvas&gt;	^#(#{UI.FullSpec} 		#window: 		#(#{UI.WindowSpec} 			#properties: #(#{UI.PropertyListDictionary} #sizeType #specifiedSize #positionType #screenCenter #openType #advanced ) 			#label: 'Administrdor de cobros' 			#min: #(#{Core.Point} 700 400 ) 			#max: #(#{Core.Point} 700 400 ) 			#bounds: #(#{Graphics.Rectangle} 330 164 1030 564 ) 			#colors: 			#(#{UI.LookPreferences} 				#setForegroundColor: #(#{Graphics.ColorValue} #black ) 				#setBackgroundColor: #(#{Graphics.ColorValue} 5734 5734 5734 ) 				#setSelectionForegroundColor: #(#{Graphics.ColorValue} #black ) 				#setSelectionBackgroundColor: #(#{Graphics.ColorValue} 4096 6143 8191 ) 				#setBorderColor: #(#{Graphics.ColorValue} #lightGray ) ) ) 		#component: 		#(#{UI.SpecCollection} 			#collection: #(				#(#{UI.TableViewSpec} 					#layout: #(#{Graphics.LayoutFrame} 8 0 64 0 689 0 316 0 ) 					#name: #Table2 					#colors: 					#(#{UI.LookPreferences} 						#setBackgroundColor: #(#{Graphics.ColorValue} #white ) ) 					#model: #aspectoTabla 					#selectionStyle: #row ) 				#(#{UI.InputFieldSpec} 					#layout: #(#{Graphics.LayoutFrame} 78 0 29 0 186 0 48 0 ) 					#name: #InputField2 					#model: #id 					#alignment: #center 					#type: #number 					#formatString: '0' ) 				#(#{UI.ActionButtonSpec} 					#layout: #(#{Graphics.LayoutFrame} 195 0 27 0 302 0 49 0 ) 					#name: #ActionButton3 					#model: #buscarCobro 					#label: 'BUSCAR' 					#style: #large 					#defaultable: true ) 				#(#{UI.ActionButtonSpec} 					#layout: #(#{Graphics.LayoutFrame} 155 0 323 0 258 0 345 0 ) 					#name: #ActionButton1 					#model: #clickAddCobro 					#label: 'Agregar' 					#style: #large 					#defaultable: true ) 				#(#{UI.ActionButtonSpec} 					#layout: #(#{Graphics.LayoutFrame} 265 0 323 0 367 0 345 0 ) 					#name: #ActionButton2 					#model: #clickModCobro 					#label: 'Modificar' 					#style: #large 					#defaultable: true ) 				#(#{UI.ActionButtonSpec} 					#layout: #(#{Graphics.LayoutFrame} 5 0 366 0 110 0 388 0 ) 					#name: #ActionButton4 					#model: #volver 					#label: 'CANCELAR' 					#style: #large 					#defaultable: true ) 				#(#{UI.ActionButtonSpec} 					#layout: #(#{Graphics.LayoutFrame} 375 0 323 0 479 0 345 0 ) 					#name: #ActionButton5 					#model: #clickElimCobro 					#label: 'Eliminar' 					#style: #large 					#defaultable: true ) 				#(#{UI.LabelSpec} 					#layout: #(#{Graphics.LayoutOrigin} 10 0 26 0 ) 					#name: #Label2 					#label: 'ID Cobro :' 					#style: #large ) ) ) )</body>
</methods>


<methods>
<class-id>AdmCobros</class-id> <category>accessing</category>

<body package="Gestor_Alquileres" selector="id">id	"This method was generated by UIDefiner.  Any edits made here	may be lost whenever methods are automatically defined.  The	initialization provided below may have been preempted by an	initialize method."	^id isNil		ifTrue:			[id := 0 asValue]		ifFalse:			[id]</body>

<body package="Gestor_Alquileres" selector="buscarCobro">buscarCobro| aux  wa |	aux:=Administrador  new.	(id value ~= 0)	ifFalse:[contenedor:= aux cobros]	ifTrue:[		wa := aux cobros detect:[:a| a id = (id value) ] ifNone: [id value: 0.		wa:=nil].		(wa isNil) ifFalse:[      contenedor:= aux cobros select:[:b| b id = wa id]]].self actualizarTabla.</body>

<body package="Gestor_Alquileres" selector="volver">volver| aux |	aux:=MenuPrincipal new.	aux open.	^self closeRequest</body>

<body package="Gestor_Alquileres" selector="clickModCobro">clickModCobro	| cobro  vc aux seleccion fila |	aux:=Administrador new.	cobro := Cobro new.	vc := VentanaCobros new. 		fila:=(aspectoTabla selectionIndex)value y.	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida'] ifFalse:[			vc cobro: cobro.	seleccion:= aspectoTabla table at:(1@fila ).	vc contCobro value:(aux buscarCobro: seleccion ) .	vc open.	]</body>

<body package="Gestor_Alquileres" selector="clickAddCobro">clickAddCobro	| cobro vc |		cobro := Cobro new.	vc := VentanaCobros new.	vc cobro: cobro.	vc open</body>

<body package="Gestor_Alquileres" selector="actualizarTabla">actualizarTabla		|coleccion |coleccion := OrderedCollection new.coleccion add: 'id'.coleccion add: 'idContrato'. coleccion add: 'fecha'. coleccion add: 'mesCorrespondiente'. coleccion add: 'añoCorrespondiente'. coleccion add: 'Monto Total'.    contenedor do: [:p | coleccion add: p id. coleccion  add: p idContrato.	coleccion  add: p fecha. coleccion  add: p mesCorrespondiente. coleccion  add: p añoCorrespondiente. coleccion  add: p montoTotal ].	tablaCobros   tableHolder value:(TwoDList 								on: coleccion								columns: 6								rows: coleccion size / 6).</body>

<body package="Gestor_Alquileres" selector="aspectoTabla">aspectoTabla	"This method was generated by UIDefiner.  Any edits made here	may be lost whenever methods are automatically defined.  The	initialization provided below may have been preempted by an	initialize method."	^aspectoTabla isNil		ifTrue:			[aspectoTabla := TableInterface new selectionInTable: tablaCobros ]		ifFalse:			[aspectoTabla]</body>

<body package="Gestor_Alquileres" selector="clickElimCobro">clickElimCobro	"This stub method was generated by UIDefiner"| aux seleccion fila|		aux:=Administrador new. 		fila:=(aspectoTabla selectionIndex)value y.	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida'] ifFalse:		[(seleccion:= aspectoTabla table at:(1@fila ))		ifNotNil:[(Dialog confirm: 'Esta seguro que quiere eliminar el cobro?')		ifTrue: [aux eliminarCobro: seleccion.			    aux guardarDatos]]].	^self buscarCobro.</body>
</methods>

