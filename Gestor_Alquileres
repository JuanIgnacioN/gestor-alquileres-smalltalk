<body package="Gestor_Alquileres" selector="buscarLocalidad">buscarLocalidad| aux  wa |	aux:=Administrador  new.	(id value ~= 0)	ifFalse:[contenedor:= aux localidades]	ifTrue:[		wa := aux localidades detect:[:a| a id = (id value) ] ifNone: [id value: 0.		wa:=nil].			(wa isNil) ifFalse:[      contenedor:= aux localidades select:[:b| b id = wa id]]].	self actualizarTabla.</body>
