<methods>
<class-id>AdmPersonas</class-id> <category>Eliminar</category>

<body package="Gestor_Alquileres" selector="clickElimPersona">clickElimPersona	"This stub method was generated by UIDefiner"| aux seleccion fila|		aux:=Administrador new. 		fila:=(aspectoTabla selectionIndex)value y.	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida'] ifFalse:		[(seleccion:= aspectoTabla table at:(3@fila ))		ifNotNil:[(Dialog confirm: 'Esta seguro que quiere eliminar la persona?')		ifTrue: [aux eliminarPersonaDNI: seleccion.			    aux guardarDatos]]].	^self buscarPersonas.</body>

<body package="Gestor_Alquileres" selector="clickExportar">clickExportar	| dataStream textoPersona nombre|	nombre:= Dialog request: 'Ingrese el nombre del archivo a exportar' initialAnswer: 'Personas'.	textoPersona := String new.	textoPersona := 'DNI' , ';' , 'Nombre' , ';' , 'Apellido' , ';'				, 'Fecha Nacimiento' , ';'				, 'Sexo' , ';'				, 'Calle' , ';'				, 'Numero' , ';'				, 'Departamento' , ';'				, 'Piso'.	dataStream := ('C:\paradigmas\',nombre,'.csv') asFilename writeStream.	dataStream nextPutAll:textoPersona.	dataStream cr.		contenedor do: 			[:e |			textoPersona := e dni printString , ';' , e nombre  , ';'						, e apellido , ';'						, e fechaNacimiento printString , ';'						, e sexo  , ';'						, e idCalle printString , ';'						, e numero printString , ';'						, e departamento printString , ';'						, e piso printString.			dataStream nextPutAll: textoPersona.			dataStream cr].	dataStream close.		Dialog warn:'Se ha exportado el archivo con exito'.</body>
</methods>

<methods>
<class-id>AdmPersonas</class-id> <category>Busquedas</category>

<body package="Gestor_Alquileres" selector="buscarInquilinos">buscarInquilinos| aux w|	aux:=Administrador  new.	w := aux personas.	contenedor:= w select: [:a| a tipo = 'Inquilino' ].	self actualizarTabla.</body>

<body package="Gestor_Alquileres" selector="buscarGarantes">buscarGarantes| aux w|	aux:=Administrador  new.	w := aux personas.	contenedor:= w select: [:a| a tipo = 'Garante' ].	self actualizarTabla.</body>

<body package="Gestor_Alquileres" selector="buscarPersonas">buscarPersonas| aux aux2|	aux:=Administrador  new.	aux2:= Administrador  new.	contenedor := aux personas.	(dni value = 0) ifFalse:   [ contenedor:= (aux2 buscarDNIPorSubcadena: (dni value))] .					self actualizarTabla.</body>

<body package="Gestor_Alquileres" selector="buscarPropietarios">buscarPropietarios| aux w|	aux:=Administrador  new.	w := aux personas.	contenedor:= w select: [:a| a tipo = 'Propietario' ].	self actualizarTabla.</body>
</methods>

<methods>
<class-id>AdmPersonas</class-id> <category>tabla</category>

<body package="Gestor_Alquileres" selector="aspectoTabla">aspectoTabla	"This method was generated by UIDefiner.  Any edits made here	may be lost whenever methods are automatically defined.  The	initialization provided below may have been preempted by an	initialize method."	^aspectoTabla isNil		ifTrue:			[aspectoTabla := TableInterface new selectionInTable: tablaPersonas ]		ifFalse:			[aspectoTabla]</body>

<body package="Gestor_Alquileres" selector="actualizarTabla">actualizarTabla		|coleccion aux|aux:=Administrador new.coleccion := OrderedCollection new.coleccion add: 'Apellido'.coleccion add: 'Nombre'.coleccion add: 'DNI'.coleccion add: 'Departamento'.coleccion add: 'Piso'.coleccion add: 'Sexo'.coleccion add: 'Fecha de Nacimiento'.coleccion add: 'Altura'. coleccion add: 'Calle'.contenedor do: [:p | coleccion add: p apellido. coleccion add: p nombre. coleccion add: p dni. coleccion add: p departamento. coleccion add: p piso. coleccion add: p sexo. coleccion add: p fechaNacimiento . coleccion add: p numero. coleccion add: (aux buscarNombreCalle: (p idCalle)) ].	tablaPersonas   tableHolder value:(TwoDList								on: coleccion								columns: 9								rows: coleccion size / 9)</body>

<body package="Gestor_Alquileres" selector="dni">dni	"This method was generated by UIDefiner.  Any edits made here	may be lost whenever methods are automatically defined.  The	initialization provided below may have been preempted by an	initialize method."	^dni isNil		ifTrue:			[dni := 0 asValue]		ifFalse:			[dni]</body>

<body package="Gestor_Alquileres" selector="dni:">dni: anObject	dni := anObject</body>
</methods>

