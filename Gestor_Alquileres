<body package="Gestor_Alquileres" selector="clickElimCalle">clickElimCalle	"This stub method was generated by UIDefiner"| aux seleccion fila|		aux:=Administrador new. 		fila:=(aspectoTabla selectionIndex)value y.	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida'] ifFalse:		[(seleccion:= aspectoTabla table at:(1@fila ))		ifNotNil:[(Dialog confirm: 'Esta seguro que quiere eliminar la calle?')		ifTrue: [aux eliminarCalle: seleccion.			    aux guardarDatos]]].	^self buscarCalle.</body>

<body package="Gestor_Alquileres" selector="clickModCalle">clickModCalle	| calle  vc aux seleccion fila|		aux:=Administrador new.	calle := Calles new.	vc := VentanaCalles new. 	fila:= (aspectoTabla selectionIndex)value y. 	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida']		ifFalse:[			vc calle: calle.			seleccion:= aspectoTabla table at:(1@fila).			vc contCalles value:(aux buscarCalle: seleccion).			vc open.			]</body>

<body package="Gestor_Alquileres" selector="buscarCalle">buscarCalle| aux  wa |	aux:=Administrador  new.	(id value ~= 0)	ifFalse:[contenedor:= aux calles]	ifTrue:[		wa := aux calles detect:[:a| a id = (id value) ] ifNone: [id value: 0.		wa:=nil].			(wa isNil) ifFalse:[      contenedor:= aux calles select:[:b| b id = wa id]]].	self actualizarTabla.</body>

<body package="Gestor_Alquileres" selector="aspectoTabla">aspectoTabla	"This method was generated by UIDefiner.  Any edits made here	may be lost whenever methods are automatically defined.  The	initialization provided below may have been preempted by an	initialize method."	^aspectoTabla isNil		ifTrue:			[aspectoTabla := TableInterface new selectionInTable: tablaCalles ]		ifFalse:			[aspectoTabla]</body>

<body package="Gestor_Alquileres" selector="actualizarTabla">actualizarTabla		|coleccion aux|aux:= Administrador new.coleccion := OrderedCollection new.coleccion add: 'ID'. coleccion add: 'Nombre'.coleccion add:'Localidad'.contenedor do: [:p | coleccion  add: p id .coleccion add: p nombre. coleccion add: (aux buscarLocalidadPorId: (p localidad))].	tablaCalles   tableHolder value:(TwoDList 								on: coleccion								columns: 3								rows: coleccion size / 3).</body>

<body package="Gestor_Alquileres" selector="clickAddCalle">clickAddCalle	| calle vc |		calle := Calles new.	vc := VentanaCalles new.	vc calle: calle.	vc open</body>
</methods>

