<body package="Gestor_Alquileres" selector="deudoresAFecha">deudoresAFecha| aux  |aux := Administrador new.contenedor:= aux deudoresAFecha.self actualizarTabla.</body>

<body package="Gestor_Alquileres" selector="volver">volver| aux |	aux:=MenuPrincipal new.	aux open.	^self closeRequest</body>
</methods>


