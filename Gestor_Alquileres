<body package="Gestor_Alquileres" selector="eliminar:de:">eliminar: unObjeto de:unaLista	unaLista remove:unObjeto.	self changed</body>

<body package="Gestor_Alquileres" selector="eliminarCobro:">eliminarCobro:unid|persona|	persona:=cobros     detect:[:p|p  id= unid].	self eliminar: persona de: cobros</body>

<body package="Gestor_Alquileres" selector="eliminarCalle:">eliminarCalle:unId|unaCalle|	unaCalle:=calles   detect:[:p|p  id= unId].	self eliminar: unaCalle de: calles</body>

<body package="Gestor_Alquileres" selector="eliminarInmueble:">eliminarInmueble:unId|aux|	aux:=inmuebles  detect:[:p|p  id= unId].	self eliminar: aux de: inmuebles.</body>

<body package="Gestor_Alquileres" selector="eliminarContrato:">eliminarContrato:unId|contrato|	contrato:=contratos  detect:[:p|p  id= unId].	self eliminar: contrato de: contratos</body>

<body package="Gestor_Alquileres" selector="eliminarPersonaDNI:">eliminarPersonaDNI: unDni|persona|	persona:=personas detect:[:p|p  dni= unDni].	self eliminar: persona de: personas</body>

<body package="Gestor_Alquileres" selector="eliminarInmueble:en:">eliminarInmueble:unaPersona en: unacalle|inmueble|	inmueble:=inmuebles  detect:[:p|(p  dni= unaPersona  dni)&amp;(p idCalle= unacalle)].	self eliminar: inmueble de: inmuebles .</body>

<body package="Gestor_Alquileres" selector="eliminarLocalidad:">eliminarLocalidad:unId|unaLocalidad|	unaLocalidad:=localidades   detect:[:p|p  id= unId].	self eliminar: unaLocalidad de: localidades</body>

<body package="Gestor_Alquileres" selector="eliminarPersona2:">eliminarPersona2:unaPersonapersonas remove: unaPersona.	"self changed"</body>

<body package="Gestor_Alquileres" selector="eliminarPersona:">eliminarPersona:unaPersona	| c|	c:= personas detect:[:j|j  dni = unaPersona dni] ifNone:[nil].	(c isNil)		ifTrue:[		unaPersona activo: false.		self actualizar: c con: unaPersona].		self agregar:unaPersona en: personas.</body>

<body package="Gestor_Alquileres" selector="eliminarInmuebleNumero:en:altura:">eliminarInmuebleNumero: nroDepto  en: unaCalle altura: unaAltura|inmueble|	inmueble:=inmuebles  detect:[:p|(p  departamento  = nroDepto)&amp;(p idCalle=(self buscarCallePorNombre:  unaCalle)&amp;( p numero = unaAltura  ) )].	self eliminar: inmueble de: inmuebles .</body>
