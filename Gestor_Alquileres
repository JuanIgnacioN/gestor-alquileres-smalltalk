<body package="Gestor_Alquileres" selector="clickModContrato">clickModContrato	| contrato vc aux seleccion fila |	aux:=Administrador new.	contrato := Contrato new.	vc := VentanaContratos new. 		fila:=(aspectoTabla selectionIndex)value y.	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida'] ifFalse:[			vc contrato: contrato.	seleccion:= aspectoTabla table at:(1@fila ).	 vc contContrato value:(aux buscarContrato: seleccion).	vc open.	]</body>

<body package="Gestor_Alquileres" selector="clickElimContrato">clickElimContrato	"This stub method was generated by UIDefiner"| aux seleccion fila|		aux:=Administrador new. 		fila:=(aspectoTabla selectionIndex)value y.	fila &lt;= 1 ifTrue:[Dialog warn: 'Seleccione una fila válida'] ifFalse:		[(seleccion:= aspectoTabla table at:(1@fila ))		ifNotNil:[(Dialog confirm: 'Esta seguro que quiere eliminar el contrato?')		ifTrue: [aux eliminarContrato: seleccion.			    aux guardarDatos]]].	^self buscarContratos.</body>

<body package="Gestor_Alquileres" selector="buscarContratos">buscarContratos| aux  wa |	aux:=Administrador  new.	(id value ~= 0)	ifFalse:[contenedor:= aux contratos]	ifTrue:[		wa := aux contratos detect:[:a| a id = (id value) ] ifNone: [id value: 0.		wa:=nil].	(wa isNil) ifFalse:[      contenedor:= aux contratos select:[:b| b id = wa id]]].self actualizarTabla.</body>

<body package="Gestor_Alquileres" selector="actualizarTabla">actualizarTabla		|coleccion  |coleccion := OrderedCollection new.coleccion add: 'ID'.coleccion add: 'idGarante'.coleccion add: 'FechaInicio'.coleccion add: 'FechaFin'.coleccion add: 'FechaResicion'.coleccion add: 'Monto'.coleccion add: 'PDP'.coleccion add: 'DLM'.coleccion add: 'idInquilino'.coleccion add: 'idInmueble'.contenedor do: [:p | coleccion add: p id. coleccion add: p idGarante. coleccion add: p fechaInicio . coleccion add: p fechaFin . coleccion add: p fechaRescision. coleccion add: p monto . coleccion add: p pdp  . coleccion add: p dlm. coleccion add: p idInquilino . coleccion add: p idInmueble ].	tablaContratos   tableHolder value:(TwoDList 								on: coleccion								columns: 10								rows: coleccion size / 10).</body>
