<class-id>AdmLocalidades</class-id> <category>accessing</category>

<body package="Gestor_Alquileres" selector="clickAddLocalidad">clickAddLocalidad	| localidad vl |		localidad := Localidad new.	vl := VentanaLocalidades new.	vl localidad: localidad.	vl open</body>

<body package="Gestor_Alquileres" selector="volver">volver| aux |	aux:=MenuPrincipal new.	aux open.	^self closeRequest</body>

<body package="Gestor_Alquileres" selector="actualizarTabla">actualizarTabla		|coleccion |coleccion := OrderedCollection new.coleccion add: 'ID'. coleccion add: 'Nombre'.coleccion add:'Departamento'.contenedor do: [:p | coleccion  add: p id .coleccion add: p nombre. coleccion add: p departamento].	tablaLocalidades   tableHolder value:(TwoDList 								on: coleccion								columns: 3								rows: coleccion size / 3).</body>

<body package="Gestor_Alquileres" selector="aspectoTabla">aspectoTabla	"This method was generated by UIDefiner.  Any edits made here	may be lost whenever methods are automatically defined.  The	initialization provided below may have been preempted by an	initialize method."	^aspectoTabla isNil		ifTrue:			[aspectoTabla := TableInterface new selectionInTable: tablaLocalidades ]		ifFalse:			[aspectoTabla]</body>

