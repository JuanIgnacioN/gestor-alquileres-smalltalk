<body package="Gestor_Alquileres" selector="clickModLocalidad">clickModLocalidad	| localidad  vl aux seleccion fila|		aux:=Administrador new.	localidad := Localidad new.	vl := VentanaLocalidades new. 		fila:= (aspectoTabla selectionIndex) value y.	fila &lt;= 1 ifTrue: [Dialog warn: 'Seleccione una fila válida']		ifFalse: [			vl localidad: localidad.			seleccion:= aspectoTabla table at:(1@fila).			vl contLocalidades value:(aux buscarLocalidad: seleccion).			vl open.			].</body>

